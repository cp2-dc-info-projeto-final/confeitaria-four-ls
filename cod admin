   $sql = "INSERT INTO administrador (CPF_admin, nome, senha) VALUES
            ('$CPF_admin','$nome','$senha')";
    if(mysqli_query($connection, $sql)){
        session_unset();
        header("Location: formulario-login.html");
        exit();
    } else{
        die("Erro $sql. " . mysqli_error($connection));
    }
   